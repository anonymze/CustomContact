<?xml version="1.0" encoding="UTF-8"?>
<database defaultIdMethod="native" name="TheliaMain" namespace="CustomContact\Model" >

    <table name="custom_contact">
        <column autoIncrement="true" name="id" primaryKey="true" required="true" type="INTEGER" />
        <column name="title" size="255" type="VARCHAR" />
        <column name="code" size="255" type="VARCHAR"/>
        <column name="field_configuration" type="LONGVARCHAR" />
        <column name="email" size="255" type="VARCHAR" />
        <column name="return_url" size="255" type="VARCHAR" />
        <column name="success_message" size="255" type="LONGVARCHAR" />
        <behavior name="timestampable" />
    </table>

  <external-schema filename="local/config/schema.xml" referenceOnly="true" />
</database>
